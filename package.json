{
    "name": "api-testing-framework",
    "version": "1.0.0",
    "description": "A robust API testing framework with TypeScript, Mocha and Supertest",
    "main": "index.js",
    "scripts": {
      "test": "mocha --require ts-node/register --reporter mochawesome --reporter-options reportDir=reports,reportFilename=report,quiet=true 'src/**/*.test.ts'",
      "test:watch": "mocha --require ts-node/register --watch --watch-files src/ 'src/**/*.test.ts'"
    },
    "keywords": [
      "api",
      "testing",
      "typescript",
      "supertest",
      "mocha"
    ],
    "author": "Your Name",
    "license": "MIT",
    "devDependencies": {
      "@types/chai": "^4.3.5",
      "@types/mocha": "^10.0.1",
      "@types/node": "^20.8.2",
      "@types/supertest": "^2.0.14",
      "@typescript-eslint/eslint-plugin": "^6.7.4",
      "@typescript-eslint/parser": "^6.7.4",
      "chai": "^4.3.7",
      "dotenv": "^16.3.1",
      "eslint": "^8.50.0",
      "mocha": "^10.2.0",
      "supertest": "^6.3.3",
      "ts-node": "^10.9.1",
      "typescript": "^5.2.2",
      "zod": "^3.22.4"
    }
  }